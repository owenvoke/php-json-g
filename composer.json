{
    "name": "pxgamer/json-g",
    "description": "A PHP implementation of JSON-G.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "RaidAndFade\\JsonG\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RaidAndFade\\JsonG\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    }
}
